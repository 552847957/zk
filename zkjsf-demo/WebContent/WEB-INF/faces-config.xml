		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>MyApplierBean</managed-bean-name>
		<managed-bean-class>
			org.zkoss.jsfdemo.test2.MyApplierBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
