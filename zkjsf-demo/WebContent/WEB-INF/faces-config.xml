		<managed-bean-class>
			org.zkoss.jsfdemo.test2.MyNavigator
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
		<managed-bean>
		<managed-bean-name>MyNavigator2</managed-bean-name>
		<managed-bean-class>
			org.zkoss.jsfdemo.test2.MyNavigator
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<converter>
		<converter-id>dateConverter</converter-id>
		<converter-class>
			org.zkoss.jsfdemo.DateConverter
		</converter-class>
	</converter>


	<!-- converter for test -->
	<converter>
		<converter-id>MyConverter</converter-id>
		<converter-class>
			org.zkoss.jsfdemo.test2.MyConverter
		</converter-class>
	</converter>

	<!-- validator for test -->
	<validator>
		<validator-id>MyStringValidator</validator-id>
