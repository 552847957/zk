<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>

	<navigation-rule>
		<from-view-id>/reservation.jsp</from-view-id>
		<navigation-case>
			<from-outcome>result</from-outcome>
			<to-view-id>/reservationResult.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/reservationResult.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/reservation.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/reservation-sp.jsp</from-view-id>
		<navigation-case>
			<from-outcome>result</from-outcome>
			<to-view-id>/reservationResult-sp.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/reservationResult-sp.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/reservation-sp.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- Navigation rule for testing -->
	<navigation-rule>
		<from-view-id>/test2/navigation.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/test2/navigation_success.jsp</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/test2/navigation.jsp</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/test2/navigation_failed.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/test2/navigation2.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/test2/navigation_success2.jsp</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/test2/navigation.jsp</from-view-id>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/test2/navigation_failed2.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>


	<managed-bean>
		<managed-bean-name>ReservationBean</managed-bean-name>
		<managed-bean-class>
			org.zkoss.jsfdemo.reservation.ReservationBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ValueBindingBean</managed-bean-name>
		<managed-bean-class>
			org.zkoss.jsfdemo.ValueBindingBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ConverterBean</managed-bean-name>
		<managed-bean-class>
			org.zkoss.jsfdemo.ConverterBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ValidatorBean</managed-bean-name>
		<managed-bean-class>
			org.zkoss.jsfdemo.ValidatorBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CommandBean</managed-bean-name>
		<managed-bean-class>
			org.zkoss.jsfdemo.CommandBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>



	<!--bean for test-->
	<managed-bean>
		<managed-bean-name>testInputboxBean</managed-bean-name>
