<!-- 20051104: Tom M. Yeh
	The locator is class loader, so we have to specify the full URI.
	Or, Tomcat won't be able to load it
<?taglib uri="/web/WEB-INF/tld/web/core.dsp.tld" prefix="c"?>
-->

<language>
	<language-name>xul/html</language-name>
	<client-type>html</client-type>
	<namespace>http://www.zkoss.org/2005/zul</namespace>
	<extension>xul</extension>
	<extension>zul</extension>

	<version>
		<version-class>org.zkoss.zul.Version</version-class>
		<version-uid>2.3.1</version-uid>
	</version>

	<!-- Note: JavaScript that are listed here will be loaded no matter
		what language is used.
		Use zk.load("x.y") if x.y is required only for certain language
	-->
	<javascript src="~./js/ext/prototype/prototype.js" charset="UTF-8"/>
	<javascript src="~./js/ext/aculo/effects.js" charset="UTF-8"/>
	<javascript src="~./js/ext/aculo/dragdrop.js" charset="UTF-8"/>
	<javascript src="~./js/zk/html/boot.js" charset="UTF-8"/>
	<javascript src="~./js/zk/html/lang/mesg*.js" charset="UTF-8"/>
	<javascript src="~./js/zk/html/common.js" charset="UTF-8"/>
	<javascript src="~./js/zk/html/au.js" charset="UTF-8"/>
	<javascript src="~./js/zk/locale.js.dsp" charset="UTF-8"/>

	<stylesheet href="~./zul/css/norm*.css.dsp*" type="text/css"/>

	<!-- To have better performance with BeanShell, import most-common later -->
	<zscript language="Java">
	import org.zkoss.zk.ui.util.Clients;
	import org.zkoss.zk.ui.event.*;
	import java.util.*;
	import java.lang.*;
	import org.zkoss.zk.ui.*;
	import org.zkoss.zul.*;

	void alert(String m) {
		Messagebox.show(m);
	}
	</zscript>

	<zscript language="JavaScript">
	importPackage(Packages.java.lang);
	importPackage(Packages.java.util);
	importPackage(Packages.org.zkoss.zul);
	importPackage(Packages.org.zkoss.zk.ui);
	importPackage(Packages.org.zkoss.zk.ui.event);
	importClass(Packages.org.zkoss.zk.ui.util.Clients);
	function alert(m) {
		Messagebox.show(m);
	}
	</zscript>

	<!-- Groovy "forgot" what are imported, so we have to import each time
	-->
	<zscript language="Groovy" each-time="true">
	import java.lang.*
	import java.util.*
	import org.zkoss.zul.*
	import org.zkoss.zk.ui.*
	import org.zkoss.zk.ui.event.*
	import org.zkoss.zk.ui.util.Clients
	</zscript>
	<zscript language="Groovy">
	alert = {m -> Messagebox.show(m)}
	</zscript>

	<zscript language="Ruby">
	require 'java'
	module Java
		include_package 'java.lang'
		include_package 'java.util'
		include_package 'org.zkoss.zul'
		include_package 'org.zkoss.zk.ui'
		include_package 'org.zkoss.zk.ui.event'
		include_class 'org.zkoss.zk.ui.util.Clients'
	end
	def alert (m)
		Java::Messagebox.show m
	end
	</zscript>

	<!-- 
		Note: '~./' means it is located in the class path starting from
		'/metainfo/web'. For example, '~./img/abc.gif' means abc.gif located
		at '/metainfo/web/img'.
	-->
	<mold>
		<mold-name>desktop</mold-name>
		<mold-uri>~./zul/html/desktop.dsp</mold-uri>
	</mold>
	<mold>
		<mold-name>page</mold-name>
		<mold-uri>~./zul/html/page.dsp</mold-uri>
	</mold>

	<!-- Defines the component and attribute names used to represent
	a label when a plain text is found in ZUL XML.
	-->
	<label-template>
		<component-name>label</component-name>
		<component-attribute>value</component-attribute>
	</label-template>
