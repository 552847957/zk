<zk>
	<label multiline="true">
		1. click the first navitem, it should be disabled about 0.8sec and then enabled again
		2. click the second navitem, it should be disabled forever
		3. click the third navitem, it should be disabled forever
		4. click the forth navitem, the forth and fifth navitem should be disabled forever

	</label>
	<navbar>
		<navitem label="autodisable 800ms" autodisable="self"/>
		<navitem label="autodisable forever" autodisable="+self"/>
		<navitem label="autodisable onClick=setDisable(true)" autodisable="self" onClick="self.setDisabled(true)"/>
		<navitem label="autodisable forever" id="n1" autodisable="+self,+n2"/>
		<navitem label="autodisable forever" id="n2" autodisable="+self"/>
	</navbar>
</zk>