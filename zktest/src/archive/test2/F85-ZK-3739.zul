<zk>
	<label>
		New feature: add InputElement.insertAtCaret(String) API.
	</label>
	<toolbar>
		<toolbarbutton label="SELECT " onClick="textbox.insertAtCaret(self.label)"/>
		<toolbarbutton label="FROM " onClick="textbox.insertAtCaret(self.label)"/>
		<toolbarbutton label="WHERE " onClick="textbox.insertAtCaret(self.label)"/>
		<toolbarbutton label="DISTINCT " onClick="textbox.insertAtCaret(self.label)"/>
	</toolbar>
	<textbox id="textbox" rows="10" width="100%" value="========================"/>
	<button label="print" onClick="print()"/>
	<zscript><![CDATA[
	public void print() {
		String val = textbox.getValue();
		alert(val);
		Clients.log(val);
	}
	]]></zscript>
</zk>
