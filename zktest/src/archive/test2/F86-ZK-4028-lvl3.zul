<?xml version="1.0" encoding="UTF-8"?>
<zk>
    <div style="border: 1px solid red">
        <label value="@init('Level '.concat(currentLevel.level))"/>
        <label value="@load(data.text)" />
        <label value="@load(context)" />

        <vlayout>
            <forEach items="@load(currentLevel.child eq null ? [] : currentLevel.child.items)">
                <a onClick="@command('levelNavTo', level=currentLevel.child, key=self.label)" label="@init(each.x)"/>
            </forEach>
        </vlayout>

        <div>
            <button onClick='currentLevel.setContext(Collections.singletonMap("test", new Random().nextInt()))' label="Random Context"/>
            <button onClick='currentLevel.navigateTo("AAA Profiles")' label="NavTo AAA Profiles"/>
            <button onClick='@command("remove", path="Configuration/Access Point/List")' label="remove List"/>
        </div>
    </div>
</zk>
