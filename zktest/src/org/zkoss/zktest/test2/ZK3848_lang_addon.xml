<language-addon>
    <addon-name>mycomp</addon-name>
    <!--<depends>zkbind</depends>-->
    <version>
        <version-class>zk.test.Version</version-class>
        <version-uid>1.0-SNAPSHOT</version-uid>
        <zk-version>8.5.1-SNAPSHOT</zk-version><!-- or later -->
    </version>
    <language-name>xul/html</language-name>
    <component>
        <component-name>mycomp</component-name> <!-- required -->
        <component-class>zk.test.Mycomp</component-class> <!-- required -->
        <extends>decimalbox</extends>
        <widget-class>mycomp.Mycomp</widget-class> <!-- required -->
        <mold>
            <mold-name>default</mold-name>
            <mold-uri>mold/mycomp.js</mold-uri>

            <!--
            == Tip in advance ==
                You don't have to specify a css.dsp if you need not ,
                but usually you need it.

                You could assign a .css file ,too.

                The css.dsp is almost as same as CSS file ,
                but have more powerful enhancement.	(like EL support.)
            ====
            -->
            <css-uri>css/mycomp.css.dsp</css-uri>
        </mold>
    </component>
</language-addon>
