<i3-comp>
	<!-- The name of this module. It must be unique -->
	<module-name>pxweb</module-name>
	<!-- Specifies what other module this depends -->
	<depends>pxcommon</depends>

	<component>
		<name>com.potix.util.resource.LabelLoader</name>
		<implementation-class>com.potix.web.util.resource.impl.LabelLoaderImpl</implementation-class>

		<!-- Optional; denoting whether it is singleton per domain.
			If not specified, singleton per system is assumed.
		<singleton-per-domain/>
		-->

		<!-- Any number of init-param is allowed.
		To use them, com.potix.comp.Component must be implemented.
		<init-param>
			<param-name>any name</param-name>
			<param-value>any value</param-value>
		</init-param>
		-->
	</component>
</i3-comp>
