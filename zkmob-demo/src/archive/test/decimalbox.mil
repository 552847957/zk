<?xml version='1.0' encoding='UTF-8'?>
<frame title="Decimalbox Demo" visible="true">
	<attribute name="onCommand">
		String id = event.getCommand().getId();
		if ("home".equals(id)) {
			MilDevice.goHome(null);
		} else {
			Executions.sendRedirect("/test/"+event.getCommand().getLabel()+".mil");
		}
	</attribute>

	<command id="home" label="Home" type="back"/>
	
	<command label="hello" type="ok"/>
	<command label="label" type="ok"/>
	<command label="onchanging" type="ok"/>
	<command label="listbox" type="ok"/>
	<command label="datebox" type="ok"/>
	<command label="intbox" type="ok"/>
	<command label="decimalbox" type="ok"/>
	
	<decimalbox label="decimalbox" value="123.456"/>
</frame>
