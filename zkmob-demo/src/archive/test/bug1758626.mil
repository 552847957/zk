<?xml version='1.0' encoding='UTF-8'?>
<frame title="Listbox demo"  visible="true"  onCommand="MilDevice.goHome(null)">
  <listbox id="lbx1" label="drop down" widthHint="expand">
    <attribute name="onCommand">
      Listitem li = new Listitem("hello");
      lbx1.appendChild(li);
    </attribute>
    <command label="add hello 1" type="ok"/>
    <command label="add hello 2" type="ok"/>
  </listbox>
  <command label="Home" type="back"/>
</frame>