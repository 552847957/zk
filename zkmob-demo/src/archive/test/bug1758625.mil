<?xml version='1.0' encoding='UTF-8'?>
<frame title="Listbox demo"  visible="true"  onCommand="MilDevice.goHome(null)">
	<listbox id="lbx1" label="drop down" mold="select" widthHint="expand">
		<attribute name="onCommand">
			self.setMold("select".equals(self.getMold()) ? "default" : "select");
		</attribute>
		<listitem label="item1"/>
		<listitem label="item2"/>
		<command label="change mold"/>
	</listbox>
	<command label="Home" type="back"/>
</frame>