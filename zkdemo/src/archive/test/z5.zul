<div>
	<zscript>int cnt = 0;</zscript>
	<label value="This is the first label."/>
	<div>
		This is 2nd label in the inner div
		<label value="This is 3nd label in the inner div"/>
		<include src="z5-1.zul" height="auto"/>
		<include src="z5-2.html" height="auto"/>
	</div>
	<label value="smartUpdate" style="border:1px solid #233;padding:0 2px"
		onClick='self.value += "!"'/>
	<label value="invalidate" style="border:1px solid #233;padding:0 2px"
		onClick='self.value += "#";self.invalidate();'/>
	<label value="append" style="border:1px solid #233;padding:0 2px"
		onClick='new Label("Append "+ ++cnt).setParent(inner)'/>
	<label value="insert" style="border:1px solid #233;padding:0 2px"
		onClick='inner.insertBefore(new Label("Insert "+ ++cnt),
			inner.children.empty ? null: inner.children.get(0))'/>
	<div id="inner" style="border:1px solid #449"/>
	End of file
</div>
