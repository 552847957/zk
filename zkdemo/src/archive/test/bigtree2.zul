	<zscript><![CDATA[
	void create(Treechildren tch, String prefix, int cnt) {
		for (int j = 1; j <= cnt; ++j) {
			Treeitem ti = new Treeitem();
			Treerow tr = new Treerow();
			new Treecell(prefix + j).setParent(tr);
			tr.setParent(ti);
			ti.setParent(tch);
		}
	}
	]]></zscript>
