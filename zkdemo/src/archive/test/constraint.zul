<window title="Custom Constraint" border="normal">
	<zscript><![CDATA[
Constraint ctt = new Constraint() {
	public void validate(Component comp, Object value) throws WrongValueException {
		if (value == null || value < 100) {
			String msg = "At least 100 must be specified";
			errmsg.setValue(msg);
			throw new WrongValueException(comp, msg);
		}
	}
	public String getValidationScript() {return null;}
	public String getErrorMessage() {return null;}
	boolean isClientComplete() {return false;}
}
	]]></zscript>
	Enter a number at least 100:
	<intbox constraint="${ctt}"/>
	<label id="errmsg"/>
</window>
