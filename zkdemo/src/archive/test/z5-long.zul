<window title="Test of a big list of groupbox">
	<zscript>
		data = new byte[600];
	</zscript>
	<groupbox width="200px" forEach="${data}">
		<caption image="/img/inet.png" label="Groupbox ${forEachStatus.index}">
			<button label="Label" onClick='self.parent.label += "!"'/>
		</caption>
		This is a group box
		<button label="a button"/>
		More in a group box
	</groupbox>
</window>
