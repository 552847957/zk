<window id="wnd" title="Path Test">
	<vbox id="box"/>
	<zscript><![CDATA[
void out(String txt) {
	new Label(txt).setParent(box);
}
String[] ps = {"../../ab", "ab/cd/../ef", "ab/../cd", "/ab/../cd", "ab/..", "/ab/.."};
for (int j = 0; j < ps.length; ++j)
	out(ps[j]+" becomes "+new Path(ps[j]));
	]]></zscript>
</window>
