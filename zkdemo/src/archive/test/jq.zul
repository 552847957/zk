<window id="main" title="Test with jq">
	<window id="sub">
		<label id="l" value="@sub @l: ${self.uuid}"/>
	</window>
	<label id="l" value="@l: ${self.uuid}"/>
	<separator bar="true"/>
	<script defer="true"><![CDATA[
	jq('@main').append(
		"'@l': "+jq('@l')[0].id
		+"<br/>@sub @l:"+jq('@l', jq('@sub'))[0].id);
	]]></script>
</window>
