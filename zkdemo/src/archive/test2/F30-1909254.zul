<window title="ifram demo" border="normal">
	<iframe id="ife" frameborder="true" scrolling="auto"
		src="http://www.google.com" width="95%" />
	<vbox>
		<button label="has Scroll bar" onClick='ife.scrolling= "yes"' />
		<button label="Not Scroll bar" onClick='ife.scrolling= "no"' />
		<button label="has frameborder" onClick='ife.frameborder= true' />
		<button label="Not frameborder"
			onClick='ife.frameborder= false' />
		<button label="has marginheight"
			onClick='ife.marginheight= "50px"' />
		<button label="Not marginheight" onClick='ife.marginheight= ""' />
		<button label="has marginwidth"
			onClick='ife.marginwidth= "50px"' />
		<button label="Not marginwidth" onClick='ife.marginwidth= ""' />

	</vbox>
</window>

