<?xml version="1.0" encoding="utf-8"?>

<zk>
	<window title="dynamic tree" border="normal" id="win">
	<zscript>
	
		import tree.*;
		
		
		HostTreeModel model = new HostTreeModel();
		HostIconTreeRenderer renderer = new HostIconTreeRenderer();
		int count = 10;
	</zscript>
		
		<tree id="tree" treeitemRenderer="${renderer}" model="${model}"  >
		</tree>
		<button label="add model process icon" onClick='model.addProcessType(0,0,count,"C");count=count+1;' />
		<button label="remove add process icon" onClick='model.removeProcessType(0,0,0,"C");count=count-1;' />
		<button label="update remove process icon" onClick='model.updateProcessType(0,0,0,"C")' />
	</window>
</zk>