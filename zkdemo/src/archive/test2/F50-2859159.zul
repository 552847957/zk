	<separator bar="true"/>
	<button label="set req attr" onClick='test.init(execution); execution.setAttribute("req", true)'/>
	<button label="set sess attr" onClick='session.setAttribute("sess", true)'/>
	<button label="set app attr" onClick='application.setAttribute("app", true)'/>
	<separator bar="true"/>
	<vbox id="inf"/>
	<zscript><![CDATA[
	Object test = new org.zkoss.zkdemo.test2.F2859159(inf);
	test.init(session);
	test.init(application);
	]]></zscript>
</window>