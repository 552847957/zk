<zk>
	Treemodel
	<window height="50%" title="Load on Demand">
	<zscript>
	
		import org.zkoss.zkdemo.test2.tree.BinaryTreeModel;
		
		ArrayList al = new ArrayList();
		int i=0;
		for(; i &lt; 100000; i++)
		{
			Object obj = ""+i;
			al.add(obj);
		}
		BinaryTreeModel btm = new BinaryTreeModel(al);
		
	</zscript>
		<tree vflex="true" height="50%" model="${btm}" id="tree" >
		</tree>
	</window>
</zk>