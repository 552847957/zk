<div>
<zscript>contacts = new String[] {"Monday"};</zscript>
<zk forEach="${contacts }">
<window context="popup_${each}" tooltip="tooltip_${each}">
	<vbox>
	<image src="/img/sun.jpg" />
	<label value="${each}"/>
	</vbox>

	<popup id="tooltip_${each}">
	<vbox>
	<label value="${each}"/>
	<label value="Some more info"/>
	<label value="And an extra line here too"/>
	</vbox>
	</popup>

	<menupopup id="popup_${each}">
		<menu label="File">
		</menu>
	</menupopup>
</window>
</zk>
</div>