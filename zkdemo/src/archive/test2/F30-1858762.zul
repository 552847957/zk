<zk>
<button label="show" >
<attribute name="onClick">
	Clients.showBusy("This loading bar will disappear after 5 sec.", true);
	timer.start();
</attribute>
</button>
<timer id="timer" delay="5000" repeats="true" running="false"
		onTimer='Clients.showBusy(null, false);self.stop();'/>
</zk>
