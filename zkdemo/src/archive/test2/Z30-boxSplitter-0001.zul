<zk>
	<window border="normal" width="800px" title="hbox and splitter">
		<hbox height="100px" width="800px">
		cdacdacdacdacdaca
		<splitter id="hsplitter" collapse="none"/>
		cdacdacdacdacacsdc
		</hbox>
		<label id="d" />
		<radiogroup>
			<attribute name="onCheck">
				hsplitter.setCollapse(self.selectedItem.label);
				d.setValue("Collapse: "+self.selectedItem.label);
			</attribute>
			<radio selected="true" label="none"/>
			<radio label="before"/>
			<radio label="after"/>
		</radiogroup>
	</window>
</zk>
